runtime: go
api_version: go1

handlers:
- url: /$
  static_files: index.html
  upload: index.html
  secure: always
- url: /static/*
  static_dir: static
  secure: always
- url: /.well-known
  static_dir: .well-known
- url: /.*
  script: _go_app
  secure: always

env_variables:
  TELEGRAM_BOT_TOKEN: --your bot token here--
  DATAMALL_ACCOUNT_KEY: --your datamall account key here--
